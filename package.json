{
  "name": "cra-application",
  "version": "0.0.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@reduxjs/toolkit": "^1.8.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^13.0.0-alpha.7",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.20.47",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/react-redux": "^7.1.23",
    "@types/styled-components": "^5.1.24",
    "bootstrap": "5.1.3",
    "formik": "^2.2.9",
    "http-proxy-middleware": "^2.0.4",
    "react": "^18.0.0",
    "react-bootstrap": "^2.2.2",
    "react-dom": "^18.0.0",
    "react-redux": "^8.0.0-beta.4",
    "react-router-dom": "^6.2.2",
    "react-scripts": "5.0.0",
    "react-toastify": "^8.2.0",
    "react-transition-group": "^4.4.2",
    "redux-saga": "^1.1.3",
    "styled-components": "^5.3.5",
    "tailwindcss": "^3.0.23",
    "twin.macro": "^2.8.2",
    "yup": "^0.32.11"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:e2e": "react-scripts test --watchAll -- --config jest.config.ts",
    "eject": "react-scripts eject",
    "new:component": "hygen new component",
    "new:page": "hygen new page",
    "storybook": "start-storybook -p 6006 -s public",
    "prepare": "husky install",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "rules": {
      "react-hooks/exhaustive-deps": "off"
    },
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.4.20",
    "@storybook/addon-essentials": "^6.4.20",
    "@storybook/addon-interactions": "^6.4.20",
    "@storybook/addon-links": "^6.4.20",
    "@storybook/builder-webpack5": "^6.4.20",
    "@storybook/manager-webpack5": "^6.4.20",
    "@storybook/node-logger": "^6.4.20",
    "@storybook/preset-create-react-app": "^4.1.0",
    "@storybook/react": "^6.4.20",
    "@storybook/testing-library": "^0.0.9",
    "@types/expect-puppeteer": "^4.4.7",
    "@types/jest-environment-puppeteer": "^5.0.0",
    "@types/puppeteer": "^5.4.5",
    "autoprefixer": "^10.4.4",
    "expect-puppeteer": "^6.1.0",
    "husky": "^7.0.4",
    "hygen": "^6.2.0",
    "jest-cucumber": "^3.0.1",
    "jest-puppeteer": "^6.1.0",
    "lint-staged": "^12.3.6",
    "postcss": "^8.4.12",
    "pptr-testing-library": "^0.7.0",
    "prettier": "^2.6.0",
    "puppeteer": "^13.5.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.1.6",
    "webpack": "^5.71.0"
  },
  "lint-staged": {
    "**/*": [
      "prettier --write --ignore-unknown .",
      "git add ."
    ]
  }
}
